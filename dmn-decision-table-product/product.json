{
  "minimumIvyVersion": "11.2.0",
  "installers": [
    {
      "id": "maven-import",
      "data": {
        "projects": [
          {
            "groupId": "com.axonivy.utils.dmn",
            "artifactId": "dmn-decision-table-demo",
            "version": "${version}",
            "type": "iar"
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    },
    {
      "id": "maven-dropins",
      "data": {
        "dependencies": [
          {
            "groupId": "com.axonivy.utils.dmn",
            "artifactId": "dmn-decision-table",
            "version": "${version}"
          }
        ],
        "repositories": [
          {
            "id": "maven.axonivy.com",
            "url": "https://maven.axonivy.com",
            "snapshots": {
              "enabled": "true"
            }
          }
        ]
      }
    }
  ]
}
